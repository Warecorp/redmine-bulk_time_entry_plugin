$('#entry_' + <%=params[:entry_id]%> + '_issues').replaceWith("<%= escape_javascript(render partial: 'issues_selector', locals: { issues: @issues, rnd: params[:entry_id]})%>");
$('#entry_' + <%=params[:entry_id]%> +'_activities').replaceWith("<%= escape_javascript(render partial: 'activities_selector', locals: { activities: (@selected_project.present? ? @selected_project.activities : []), rnd: params[:entry_id]})%>");
